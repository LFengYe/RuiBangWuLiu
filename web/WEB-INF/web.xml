<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <listener-class>com.cn.listener.StartInit</listener-class>
    </listener>
    <filter>
        <filter-name>CharactorFilter</filter-name>
        <filter-class>com.cn.filter.CharactorFilter</filter-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>CharactorFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>DataInterface</servlet-name>
        <servlet-class>com.cn.servlet.DataInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportInterface</servlet-name>
        <servlet-class>com.cn.servlet.ReportInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BaseInterface</servlet-name>
        <servlet-class>com.cn.servlet.BaseInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OutInterface</servlet-name>
        <servlet-class>com.cn.servlet.OutInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MoveInterface</servlet-name>
        <servlet-class>com.cn.servlet.MoveInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InInterface</servlet-name>
        <servlet-class>com.cn.servlet.InInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AppInterface</servlet-name>
        <servlet-class>com.cn.servlet.AppInterface</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFileServlet</servlet-name>
        <servlet-class>com.cn.servlet.UploadFileServlet</servlet-class>
    </servlet>
    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ContainerServlet</servlet-name>
        <servlet-class>com.cn.servlet.ContainerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>com.cn.test.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppInterface</servlet-name>
        <url-pattern>/app.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFileServlet</servlet-name>
        <url-pattern>/uploadFile.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataInterface</servlet-name>
        <url-pattern>/action.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportInterface</servlet-name>
        <url-pattern>/report.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BaseInterface</servlet-name>
        <url-pattern>/base.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OutInterface</servlet-name>
        <url-pattern>/out.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoveInterface</servlet-name>
        <url-pattern>/move.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InInterface</servlet-name>
        <url-pattern>/in.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContainerServlet</servlet-name>
        <url-pattern>/container.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            300
        </session-timeout>
    </session-config>
</web-app>
